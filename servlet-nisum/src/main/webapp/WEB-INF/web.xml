<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
                             http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

  <context-param>
    <param-name>trainer</param-name>
    <param-value>Shiv Sunder Dash</param-value>
  </context-param>

  <servlet>
    <servlet-name>welcome</servlet-name>
    <servlet-class>com.nisum.WelcomeServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>welcome</servlet-name>
    <url-pattern>/welcome</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>display</servlet-name>
    <servlet-class>com.nisum.DisplayServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>display</servlet-name>
    <url-pattern>/display</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>getpostdemo</servlet-name>
    <servlet-class>com.nisum.GetPostDemoServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>getpostdemo</servlet-name>
    <url-pattern>/getpostdemo</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>datetime</servlet-name>
    <servlet-class>com.nisum.DateTimeServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>datetime</servlet-name>
    <url-pattern>/datetime</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>cookie</servlet-name>
    <servlet-class>com.nisum.CookieServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>cookie</servlet-name>
    <url-pattern>/cookie</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>config</servlet-name>
    <servlet-class>com.nisum.ConfigServlet</servlet-class>
    <init-param>
      <param-name>companyName</param-name>
      <param-value>Nisum India Pvt Ltd</param-value>
    </init-param>
  </servlet>

  <servlet-mapping>
    <servlet-name>config</servlet-name>
    <url-pattern>/config</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>context</servlet-name>
    <servlet-class>com.nisum.ContextServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>context</servlet-name>
    <url-pattern>/context</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>com.nisum.LoginServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>redirect</servlet-name>
    <servlet-class>com.nisum.RedirectServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>redirect</servlet-name>
    <url-pattern>/redirect</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>headers</servlet-name>
    <servlet-class>com.nisum.HeaderDisplayServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>headers</servlet-name>
    <url-pattern>/headers</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>cart</servlet-name>
    <servlet-class>com.nisum.CartServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>cart</servlet-name>
    <url-pattern>/cart</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>setsession</servlet-name>
    <servlet-class>com.nisum.SessionSetServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>setsession</servlet-name>
    <url-pattern>/setsession</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>readsession</servlet-name>
    <servlet-class>com.nisum.SessionReadServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>readsession</servlet-name>
    <url-pattern>/readsession</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>submitfeedback</servlet-name>
    <servlet-class>com.nisum.FeedbackServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>submitfeedback</servlet-name>
    <url-pattern>/submitfeedback</url-pattern>
  </servlet-mapping>

  <filter>
    <filter-name>LoggingFilter</filter-name>
    <filter-class>com.nisum.LoggingFilter</filter-class>
  </filter>

  <filter-mapping>
    <filter-name>LoggingFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>

  <servlet>
    <servlet-name>step1</servlet-name>
    <servlet-class>com.nisum.Step1Servlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>step1</servlet-name>
    <url-pattern>/step1</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>step2</servlet-name>
    <servlet-class>com.nisum.Step2Servlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>step2</servlet-name>
    <url-pattern>/step2</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>forward</servlet-name>
    <servlet-class>com.nisum.ForwardingServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>forward</servlet-name>
    <url-pattern>/forward</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>target</servlet-name>
    <servlet-class>com.nisum.TargetServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>target</servlet-name>
    <url-pattern>/target</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>validate</servlet-name>
    <servlet-class>com.nisum.ValidationServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>validate</servlet-name>
    <url-pattern>/validate</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>login18</servlet-name>
    <servlet-class>com.nisum.Login18Servlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>login18</servlet-name>
    <url-pattern>/login18</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>dashboard</servlet-name>
    <servlet-class>com.nisum.DashboardServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>dashboard</servlet-name>
    <url-pattern>/dashboard</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>logout18</servlet-name>
    <servlet-class>com.nisum.Logout18Servlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>logout18</servlet-name>
    <url-pattern>/logout18</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>students</servlet-name>
    <servlet-class>com.nisum.StudentServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>students</servlet-name>
    <url-pattern>/students</url-pattern>
  </servlet-mapping>

  <display-name>Servlet Nisum Web Application</display-name>
</web-app>
